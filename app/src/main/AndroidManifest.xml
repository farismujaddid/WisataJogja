<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.aclass.wisatajogja">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:name="android.support.multidex.MultiDexApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="Faris Cah Jogja"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MenuAwal">
        </activity>
        <activity android:name=".GaleriAdapter">
        </activity>
        <activity android:name=".GaleriWisata">
        </activity>
        <activity android:name=".VideoWisata">
        </activity>

        <meta-data
                android:name="com.google.android.geo.API_KEY"
                android:value="@string/kunci_api">
        </meta-data>

        <activity android:name=".PetaWisata">
        </activity>
        <activity
                android:name=".SplashScreen"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".WebWisata">
        </activity>
        <activity android:name=".About">
        </activity>
        <activity android:name=".Home">
        </activity>
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>